import com.fasterxml.jackson.annotation.JsonIgnoreProperties;

@JsonIgnoreProperties(ignoreUnknown = true) // This allows extra fields to be ignored
public class ApiConfig {
    private String apiType;
    private String requestScope;
    private boolean allowEmptyRequestBody;
    private String mapType;
    private UrlConfig urlConfig;
    private RequestObjectConfig requestObject;
    private ResponseFormatConfig responseFormat;

    // Getters and setters
}
